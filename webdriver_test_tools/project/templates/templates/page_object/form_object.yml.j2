# FormObject YAML Documentation: 
# https://connordelacruz.com/webdriver-test-tools/yaml.html#formobjects

# REPLACE <PLACEHOLDER VALUES> BELOW AND ADD ITEMS TO THE inputs LIST:

form:
  # FORM ATTRIBUTES ------------------------------------------------------------
  # REQUIRED: Locator for the form
  form_locator:
    by: <BY>
    locator: <LOCATOR>
  # REQUIRED: Locator for the submit button
  submit_locator:
    by: <BY>
    locator: <LOCATOR>
  # FORM INPUTS ----------------------------------------------------------------
  # REQUIRED: List of inputs in the form
  inputs:
    # Example syntax:
    # 
    # - name: <INPUT NAME>
    #   <KEY>: <VALUE>
    #   ...
    # - name: <ANOTHER INPUT NAME>
    #   ...

